/*-----------------------------------------------------------------------------------*/
/*	Tab Styles
/*-----------------------------------------------------------------------------------*/

.thirtyjin-tabs.ui-tabs .ui-tabs-hide {
    position: absolute;
    left: -10000px;
	}

.thirtyjin-tabs {
	background: none;
	margin: 0 0 20px 0;
	}

.thirtyjin-tabs ul.thirtyjin-nav {
	list-style: none!important;
	margin: 0!important;
	padding: 0;
	background: none;
	border: 0;
	float: none;
	}

.thirtyjin-tabs ul.thirtyjin-nav li {
	float: left;
	position: relative;
	margin: 0 2px -1px 0!important;
	z-index: 10;
	list-style: none!important;
	}
	

.thirtyjin-tabs ul.thirtyjin-nav li a {
	border: 1px solid #dfdfdf!important;
	border-bottom: none!important;
	display: block;
	overflow: hidden;
	padding: 5px 10px 0 10px;
	height: 26px;
	background: #FBFBFB;
	margin: 0!important;
	text-decoration: none;
	
	-webkit-border-radius: 3px 3px 0 0;
	   -moz-border-radius: 3px 3px 0 0;
	   		border-radius: 3px 3px 0 0;
	}
	
.thirtyjin-tabs ul.thirtyjin-nav li a:hover {
	background: #fff!important;
	margin: 0!important;
	}

.thirtyjin-tabs ul.thirtyjin-nav li.ui-tabs-selected a {
	height: 27px;
	background: #fff;
	}

.thirtyjin-tabs .thirtyjin-tab {
	background: #fff;
	padding: 10px;
	border: 1px solid #dfdfdf;
	}

	
	

