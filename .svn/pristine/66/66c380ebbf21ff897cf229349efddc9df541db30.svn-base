/*-----------------------------------------------------------------------------------*/
/*	image style shortcode
/*-----------------------------------------------------------------------------------*/

.image_style {
	margin-bottom: 10px;
	padding: 5px;
	background: url("../images/banner_gray_bg_12px.png") repeat transparent;	
	display: block;

	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;

	-webkit-box-shadow: 0 1px 2px #ccc;
	-moz-box-shadow: 0 1px 2px #ccc;
	box-shadow: 0 1px 2px #ccc;

}
.image_style.aligncenter .image_frame,
.image_style.aligncenter .image_shadow{
	margin: 0 auto;
}
.image_frame {
	background:url("../images/loading.gif") transparent no-repeat 50% 50% ;
	position: relative;
}
.image_style,
.image_frame,
.image_style img {
	display: block;
}

.image_frame a {
	display: block;
	background: transparent ;
}

.image_frame img {
	visibility: visible;
	display: block;
	padding: 0;
	margin: 0 auto;
	box-shadow: 0 1px 3px rgba(0, 0, 0, .12);
}

.image_frame:hover img { 
	opacity: .2;
	-webkit-transition: opacity .2s linear;
	-moz-transition: opacity .2s linear;
	-o-transition: opacity .2s linear;
	-ms-transition: opacity .2s linear;
	transition: opacity .2s linear;

}

.image_frame  .image_size_small img {max-height: 150px; max-width: 150px;}
.image_frame  .image_size_medium img {max-height: 300px; max-width: 300px;}
.image_frame  .image_size_large img {max-height: 680px; max-width: 680px;}

.image_frame .image_overlay {
	position:absolute;
	top:0;
	left:0;
	height:100%;
	width:100%;
	visibility:hidden;
	overflow: hidden; 
}
.image_frame:hover .image_overlay {
	visibility: visible;
}

.image_icon_play .image_overlay {
	background:url("images/image_style/image_icon_play.png") no-repeat scroll 50% 50% transparent;
}
.image_icon_image .image_overlay {
	background:url("images/image_style/image_icon_image.png") no-repeat scroll 50% 50% transparent;
}
.image_icon_link .image_overlay {
	background:url("images/image_style/image_icon_link.png") no-repeat scroll 50% 50% transparent;
}
.image_icon_view .image_overlay {
	background:url("images/image_style/image_icon_view.png") no-repeat scroll 50% 50% transparent;
}
.image_style.alignleft {
	margin-right:16px;
}
.image_style.alignright {
	margin-left:16px;
}
.image_no_link {
	cursor: default;
}

.image_caption_title {
	display: block;
	margin: 0;
	padding: 5px 0 0;
}



/*-----------------------------------------------------------------------------------*/
/*	recent articles Shortcode
/*-----------------------------------------------------------------------------------*/

.recent_articles {
	display: block;
}

.clients-header,
.recent_articles-header {
	clear: both;
	display: block;
	padding: 0 0 10px 0;
	margin: 0 0 20px;
	background: url("../images/banner_gray_bg.png") repeat-x bottom transparent;
}
.clients-header h4,
.recent_articles-header h4{
	padding-bottom: 15px;
	line-height: 1.125em;
}
.clients-title,
.recent_articles-title {
	text-transform: uppercase;
	text-shadow: 1px 1px 0px rgba(255, 255, 255, 1.0);	
	display: inline-block;	
}

/* 
	Next/prev/more buttons 
*/


.next-prev-nav { 
	display: block;	
	float:right;
	padding-top: 15px;
}

.recent_articles .more,
.recent_articles .next,
.recent_articles .prev {
	float: left;
	background: url(./images/pagination.png) no-repeat;
	width: 20px;
	height: 20px;
	display:block;
	text-indent: -9999em;
	/* transition: all 0.4s ease-in-out;
	-moz-transition: all 0.4s ease-in-out;
	-webkit-transition: all 0.4s ease-in-out; */
}



.recent_articles .prev {
	background-position: 0 0;
	margin-right: 5px;
}
.recent_articles a.prev:hover {
	background-position: 0 -22px;
}

.recent_articles .next {
	background-position: -22px 0;
	margin-right: 5px;
}
.recent_articles a.next:hover {
	background-position: -22px -22px;
}


.recent_articles .more {
	background-position: -44px 0;	
	margin-right: 5px;
}
.recent_articles a.more:hover {
	background-position: -44px -22px;
}


.recent_articles ul {
	margin: 0;
	padding: 0;
	list-style: none;
	display: block;
	overflow: hidden;
	font-size: 0; /* For inline-block the Spacing Issue */
	margin-right: -30px;	
}

.recent_articles li {
	width: 300px;
	background-color: #fff;	
	padding: 0;
	margin: 0 10px 20px;
	display: inline-block;
	vertical-align: top;
	font-size: 16px;
	border-radius: 0px;
   -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
   box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
}

/* .recentportfolio li:first-child {
	margin: 0 0 20px 0;
} */


.recent_articles-content .widget-entry-title {
	width:auto;
	margin: 15px 0 5px;
	padding: 0 10px;
	text-align: center;
	overflow: hidden;
}
.recent_articles-content a{
	display: block;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	text-align: center;
}


.recent_articles-columns-2,
.recent_articles-columns-3,
.recent_articles-columns-4 {
	margin: 0px -10px;
	overflow: hidden;	
}

.recent_articles-columns-3 li {
	width: 300px;
}

.recent_articles-columns-4 li {
	width: 220px;
}

.recent_articles-columns-2 li {
	width: 460px;
}


.recent_articles-columns-2 .post_thumb .thumb_img {
	width: 460px;
	height: 230px;
}
.recent_articles-columns-3 .post_thumb .thumb_img {
	width: 300px;
	height: 150px;
}
.recent_articles-columns-4 .post_thumb .thumb_img {
	width: 220px;
	height: 110px;
}

.recent_articles-content .post_thumb {
	text-align: center;
	position: relative;
	margin: 0 auto 15px;
}

.recent_articles-content .post-thumb a {
	display: block;	
}


.recent_articles-content .post-thumb img {
	max-width: 100%;
	display: block;
	padding: 0;
	margin: 0 auto;
	box-shadow: 0 1px 3px rgba(0, 0, 0, .12);
}



.recent_articles-content .post_thumb .post-thumb-overlay { 
	visibility: hidden;
    display: none;
    position: absolute;

	opacity: 0;
	-moz-opacity: 0;
	filter:alpha(opacity=0);

	-webkit-transition: opacity 150ms ease-in-out;
	-moz-transition: opacity 150ms ease-in-out;
	-o-transition: opacity 150ms ease-in-out;
	transition: opacity 150ms ease-in-out;
}

.recent_articles-content .post_thumb a:hover .post-thumb-overlay { 
	background: rgba(242, 101, 53, 0.8) url(images/image_style/image_icon_view.png) 50% 50% no-repeat;
    display: block;
	position: absolute;
    top: 0;
    left: 0;
	width: 100%;
	height: 100%;
    z-index: 99;
	text-indent: -999em;
	visibility: visible;

	opacity: 1;
	-moz-opacity: 1;
	filter:alpha(opacity=1);

}


.recent_articles-content .post_thumb img { 
	margin: 0 auto;
}


.recent_articles-content .entry-title {
	font-size: 1.125em;
}

.recent_articles-content .entry-title-meta {
	
}

.recent_articles-content .post_excerpt,
.recent_articles-content .entry-summary {
	padding:10px;
}




