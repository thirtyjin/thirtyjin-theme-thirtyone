/* IE/Win - Fix animation bug - #4615 */
.ui-accordion { width: auto; }
.ui-accordion .ui-accordion-header { cursor: pointer; position: relative; margin-top: 1px; zoom: 1; }
.ui-accordion .ui-accordion-li-fix { display: inline; }
.ui-accordion .ui-accordion-header-active { border-bottom: 0 !important; }
.ui-accordion .ui-accordion-header a { display: block; font-size: 1em; padding: .1em .5em .1em .7em; }
.ui-accordion-icons .ui-accordion-header a { padding-left: 1.5em; }
/* .ui-accordion .ui-accordion-header .ui-icon { position: absolute; left: .5em; top: 50%; margin-top: -8px; } */
.ui-accordion .ui-accordion-content { padding: 1em 2.2em; border-top: 0; margin-top: -2px; position: relative; top: 1px; margin-bottom: 2px; overflow: auto; display: none; zoom: 1; }
.ui-accordion .ui-accordion-content-active { display: block; }


.thirtyjin_accordion {
	border:1px solid #ddd;
	margin-bottom:20px;
}

.thirtyjin_accordion .accordion_title {
	color:#666;
	background-color:#f5f5f5;
	margin:0;
	padding:5px 15px;
	font-size:14px;
	font-weight:normal;
	border:1px solid #fff;
	border-bottom:none;
	cursor:pointer;
	border-bottom:1px solid #ddd;
}

.thirtyjin_accordion .accordion_content {
	border-bottom:1px solid #ddd;
	padding:10px 15px;
	font-size:12px;
	overflow: hidden;
	background-color: white;
}

.thirtyjin_accordion .accordion_content:last-child {
	border-bottom:none;
}

	
.thirtyjin_accordion span.ui-icon {
    background: url(images/toggles/toggle-closed.png) no-repeat;
    width: 12px;
    height: 12px;
    position: absolute;
    top: 50%;
    left: 10px;
    margin: -6px 0 0 0;
	}

.thirtyjin_accordion .ui-state-active .ui-icon { background: url(images/toggles/toggle-open.png) no-repeat; }
	
.thirtyjin_accordion .ui-state-active {
	background-color: white;
}



/*-----------------------------------------------------------------------------------*/
/*	Toggle Styles
/*-----------------------------------------------------------------------------------*/


.thirtyjin-toggle { 
	margin: 0 0 1em 0;
	border: 1px solid #ddd;
}



.thirtyjin-toggle .thirtyjin-toggle-title {
	display: block;
	outline: none;
	position: relative;

	color:#666;
	background-color: whiteSmoke;
	padding:5px 15px;
	font-size:14px;
	font-weight:normal;
	cursor:pointer;
	} 
	

.toggle-one,
.thirtyjin-toggle .toggle-one {
	background: whiteSmoke;
	} 

.toggle-two,
.thirtyjin-toggle .toggle-two {
	background: none;
	border: none;
}
	
.thirtyjin-toggle .toggle-one:hover { }
.thirtyjin-toggle .toggle-two:hover { border: none; }

.thirtyjin-toggle .thirtyjin-toggle-inner {
	padding: 10px;
	border-top: none;
	}
.thirtyjin-toggle .inner-toggle-one {
	background: #fff;
	}
.thirtyjin-toggle .inner-toggle-two {
	background: none;
	border: none;
	}
	
.thirtyjin-toggle span.ui-icon {
    background: url(images/toggles/toggle-closed.png) no-repeat;
    width: 12px;
    height: 12px;
    position: absolute;
    top: 50%;
    left: 10px;
    margin: -6px 0 0 0;
	}

.thirtyjin-toggle .ui-state-active .ui-icon { background: url(images/toggles/toggle-open.png) no-repeat; }
.toggle-one .ui-state-active { background-color: white; }	
.toggle-one .ui-state-active { background-color: none; }	
	
