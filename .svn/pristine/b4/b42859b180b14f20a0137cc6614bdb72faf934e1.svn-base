/*-----------------------------------------------------------------------------------*/
/*	13.	Isotope Stylings
/*-----------------------------------------------------------------------------------*/

.isotope {
	margin-right: -30px;
	margin-top:30px;
	font-size: 0;
}

/* clear fix */
.isotope:after {
  content: '';
  display: block;
  clear: both;
}

.one-column .isotope    {margin-right: 0;}
.two-columns .isotope,
.three-columns .isotope,
.four-columns .isotope  {margin-right: -30px;}

/* self-clear floats */

.isotope:after {
	content: "."; 
    display: block; 
    height: 0; 
    clear: both; 
    visibility: hidden;
}

.one-column .isotope-item,
.four-columns .isotope-item,
.three-columns .isotope-item,
.two-columns .isotope-item,
.isotope-item{
	z-index: 2;
	margin: 0 20px 20px 0;
	padding: 0px;
	width: 300px;
	display: inline-block;
	vertical-align: top;
	background-color: #fff;	
	font-size: 16px;   /* don't modify to em */
   -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
   box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
}

/* For the two-columns portfolio page  */
.one-column .isotope-item    {width: 940px;}
.two-columns .isotope-item   {margin-right: 20px;width: 460px;}
.three-columns .isotope-item {margin-right: 20px;width: 300px;}
.four-columns .isotope-item  {margin-right: 20px;width: 220px;}

.isotope p{ margin-bottom: 0.5em; }
.isotope a{ 
	text-decoration: none;
	font-size: 0.875em;
	font-weight: normal;
	margin: 0;
}
.isotope img{ margin: 0px; text-align: center; max-width: 300px; max-height:225px; vertical-align: bottom; margin: 0 auto;}
.one-column .isotope img    {	max-width: 940px; max-height:735px; }
.two-columns .isotope img   {	max-width: 460px; max-height:353px; }
.three-columns .isotope img   {	max-width: 300px; max-height:225px; }
.four-columns .isotope img  {	max-width: 220px; max-height:165px; }

.four-columns .isotope-item .post-thumb,
.two-columns .isotope-item .post-thumb,
.three-columns .isotope-item .post-thumb,
.isotope-item .post-thumb {
	padding: 0px;
	text-align: center;
	position: relative;
}

.one-column .isotope-item .post-thumb {
	padding: 0px;
}

.four-columns .isotope-item .post-thumb img,
.two-columns .isotope-item .post-thumb img,
.three-columns .isotope-item .post-thumb img,
.one-column .isotope-item .post-thumb img,
.isotope-item .post-thumb img {
    -webkit-transition: opacity .2s linear;
	   -moz-transition: opacity .2s linear;
	     -o-transition: opacity .2s linear;
	    -ms-transition: opacity .2s linear;
	        transition: opacity .2s linear;
}



.portfolio_post_thumb .overlay {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(242, 101, 53, 0.8);
	font-size: 0.875em;
	font-weight: normal;
	vertical-align: middle;
	text-align: center;
	opacity: 0;
	-moz-opacity: 0;
	filter:alpha(opacity=0);
	-webkit-transition: opacity 150ms ease-in-out;
	-moz-transition: opacity 150ms ease-in-out;
	-o-transition: opacity 150ms ease-in-out;
	transition: opacity 150ms ease-in-out;	
}

.portfolio_post_thumb .overlay_text {
	color: #fff;	
	position: relative;
	top: 50%;
}

.portfolio_post_thumb a:hover .overlay {
	opacity: 1;
	-moz-opacity: 1;
	filter:alpha(opacity=1);
}




.isotope-item a{
	display: block;
}

.isotope-item .post-thumb a{
	display: block;
	background: url("images/empty.png") repeat;
}


.isotope-item .post-thumb:hover img { opacity: .9; }


.isotope-item .post-thumb:hover .post-thumb-overlay { 
	background: url(shortcodes/images/image_style/image_icon_view.png) 50% 50% no-repeat;
