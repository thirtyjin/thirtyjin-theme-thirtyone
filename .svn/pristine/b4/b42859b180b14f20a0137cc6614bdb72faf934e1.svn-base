/* =Navigation
-------------------------------------------------------------- */

#content postnav {
	clear: both;
	overflow: auto;
	padding: 0;
	margin: 0 auto;
	display: block;
}
#content postnav a {
	font-size: 0.875em;
	font-weight: normal;
	line-height: 1.2em;
	padding: 5px 2em;
}

.nav-previous,
.nav-next  {
	width: 50%;
	margin: 5px auto 15px;
}
.nav-previous  {
	float: left;
}
.nav-next {
	float: right;
}
#content nav .meta-nav {
	font-weight: normal;
}


/* Singular navigation */
#nav-single {
	position: relative;
	top: -0.3em;
	text-align: right;
	z-index: 1;
}
#content nav a,
#nav-single a{
	color: #666;
	text-transform: capitalize;
	font-size: 0.875em;
	line-height: 1em;
	font-weight: normal;
	padding: 5px 2em;
	margin: auto 0;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	-webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
    -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
	background:  #fff;
}

#content nav a:hover,
#nav-single a:hover{
	color: #fff;
	background: #F26535;
}


#nav-single .nav-previous a:before,
#nav-single .nav-next a:after {
	display: inline-block;
	font-size: 12px;
	margin-left: 5px;
	margin-right: 0px;	
}

#nav-single .nav-previous a:before {
	margin-right: 5px;	
	margin-left: 0px;	
	content: "\e628"!important;	
}


#nav-single .nav-previous,
#nav-single .nav-next {
	width: auto;
}


