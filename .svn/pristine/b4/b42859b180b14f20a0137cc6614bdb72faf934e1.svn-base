/* 
	Next/prev buttons 
*/

.gallery-slider .next,
.gallery-slider .prev {
	opacity:0;
	position:absolute;
	text-indent: -9999em;
	width:110px;
	height:100%;
	display:block;
	z-index:101;
	transition: all 0.4s ease-in-out;
	-moz-transition: all 0.4s ease-in-out;
	-webkit-transition: all 0.4s ease-in-out;
}

.gallery-slider a.next:hover,.gallery-slider a.prev:hover {
	opacity:1;
}

.gallery-slider .next {
	float: right;
	top: 0px;
	right: 0px;
	background:url(images/slideshow_arrow_right_nor.png) center no-repeat;
}

.gallery-slider .prev {
	float: left;
	top: 0px;
	left: 0px;
	background:url(images/slideshow_arrow_left_nor.png) center no-repeat;
}

.gallery-slider .pager {
	width: 100%;
	text-align: center;
	margin: 8px auto 0;
}
.gallery-slider .pager a {
	display: inline-block;
	width: 12px;
	height: 12px;	
	margin: 0 5px;
	text-decoration: none;
	background: #FFF url("images/banner_black_bg.png") repeat;
	/* background: #FFF url("images/banner_gray_bg_12px.png") repeat; */


	-webkit-box-shadow: 0 1px 3px rgba(0,0,0,.12);
       -moz-box-shadow: 0 1px 3px rgba(0,0,0,.12);
         -o-box-shadow: 0 1px 3px rgba(0,0,0,.12);
            box-shadow: 0 1px 3px rgba(0,0,0,.12);

	-webkit-border-radius: 6px;
	   -moz-border-radius: 6px;
	     -o-border-radius: 6px;
	        border-radius: 6px;
}
.gallery-slider .pager a span {
	display: none;
}
.gallery-slider .pager a.selected {
	text-decoration: underline;
	background: #F26535 url("images/banner_orange_bg.png") repeat;
	
}
